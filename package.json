{
  "name": "dispatch",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "imports": {
    "#index": "./src/index.ts",
    "#db": "./src/db/drizzleORM.export.ts",
    "#route/*": "./src/route/*",
    "#middleware": "./src/utils/middleware/index.ts",
    "#types": "./src/utils/types.ts",
    "#factory": "./src/utils/factory.ts"
  },
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "type": "bunx tsc",
    "generate": "drizzle-kit generate --config=src/db/drizzle.config.ts",
    "migrate": "drizzle-kit migrate --config=src/db/drizzle.config.ts",
    "migrate:undo": "drizzle-kit migrate:undo --config=src/db/drizzle.config.ts",
    "migrate:status": "drizzle-kit migrate:status --config=src/db/drizzle.config.ts",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "drizzle-kit": "^0.31.7",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hono/standard-validator": "^0.2.0",
    "@hono/typebox-validator": "^1.1.0",
    "@hono/zod-validator": "^0.7.5",
    "@scalar/hono-api-reference": "^0.9.25",
    "@sinclair/typebox": "^0.34.41",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-typebox": "^0.3.3",
    "hono": "^4.10.6",
    "hono-openapi": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "pino": "^10.1.0",
    "postgres": "^3.4.7"
  }
}
